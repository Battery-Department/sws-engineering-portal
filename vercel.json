{
  "framework": "nextjs",
  "buildCommand": "prisma generate && npm run build",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "outputDirectory": ".next",
  "env": {
    "NODE_ENV": "production",
    "QUIZ_ENABLED": "true",
    "ENABLE_QUIZ_INTELLIGENCE": "true",
    "ENABLE_BEHAVIORAL_TRACKING": "true",
    "ENABLE_AI_INSIGHTS": "true",
    "QUIZ_ANALYTICS_ENABLED": "true",
    "QUIZ_SESSION_TIMEOUT": "1800000",
    "QUIZ_COMPLETION_THRESHOLD": "0.8",
    "QUIZ_RETENTION_DAYS": "90",
    "QUIZ_SAMPLE_DATA_MODE": "true"
  },
  "functions": {
    "src/app/api/quiz/intelligence/live/route.ts": {
      "maxDuration": 30
    },
    "src/app/api/quiz/intelligence/metrics/route.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/quiz",
      "destination": "/quiz"
    },
    {
      "source": "/quiz/:path*",
      "destination": "/quiz/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/quiz/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, must-revalidate"
        }
      ]
    }
  ]
}